{
    "name": "glsl-canvas",
    "displayName": "glsl-canvas",
    "description": "Live WebGL preview of GLSL shaders",
    "version": "0.1.1",
    "publisher": "circledev",
    "license": "UNLICENSED",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:glsl-canvas.showGlslCanvas"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "properties": {
                "glsl-canvas.textures": {
                    "type": "object",
                    "default": {
                        "0": "",
                        "1": ""
                    },
                    "description": "The texture channels (u_texture_0, u_texture_1, ...) using http:// or file:// protocol."
                }
            }
        },
        "commands": [{
            "command": "glsl-canvas.showGlslCanvas",
            "title": "Show glslCanvas"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^4.1.0",
        "gulp-coffee": "^3.0.2",
        "gulp-cssmin": "^0.2.0",
        "gulp-html2js": "^0.4.2",
        "gulp-if": "^2.0.2",
        "gulp-livereload": "^3.8.1",
        "gulp-plumber": "^1.2.0",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^3.1.0",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-uglify": "^3.0.0",
        "gulp-watch": "^5.0.0",
        "gulp-webserver": "^0.9.1",
        "merge-stream": "^1.0.1",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "icon": "resources/icon.png",
    "repository": {
        "type": "git",
        "url": "git://github.com/actarian/vscode-glsl-canvas.git"
    }
}